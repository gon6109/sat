<Window x:Class="SatUI.ProgressDialog"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:SatUI"
        mc:Ignorable="d"
        Title="ProgressDialog" Height="100" Width="250" ResizeMode="NoResize" Closed="Window_Closed">
    <Grid>
        <ProgressBar x:Name="progress" Margin="5,20" ValueChanged="Progress_ValueChanged"/>
    </Grid>
</Window>
